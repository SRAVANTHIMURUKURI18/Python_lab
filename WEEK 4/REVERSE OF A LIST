l=[]
n=int(input("enter the no.of elements"))
for i in range(n):
     el=int(input("enter the ele  "))
     l.append(el)
print(l)

l.reverse()
print("reversed list",l)





or




l=[]
n=int(input("enter the no.of elements"))
for i in range(n):
     el=int(input("enter the ele  "))
     l.append(el)
print(l)

print(l[::-1])
